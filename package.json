  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/dockerode": "^3.3.1",
    "@types/node": "^17.0.16",
    "@types/tar": "^6.1.1",
    "eslint": "^8.8.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "commander": "^9.0.0",
    "dockerode": "^3.3.1",
    "etcd3": "^1.1.0",
